  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "airbnb-typescript"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": "off",
      "react/require-default-props": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "react/forbid-prop-types": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "prefer-promise-reject-errors": "off",
      "jsx-a11y/label-has-for": "off",
      "import/no-cycle": "off",
      "implicit-arrow-linebreak": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/static-property-placement": "off",
      "react/jsx-fragments": "off",
      "react/jsx-props-no-spreading": "off",
      "react/no-did-update-set-state": "off",
      "prefer-object-spread": "off",
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "sliceState"
          ]
        }
      ],
      "@typescript-eslint/no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "arrow-parens": [
        "error",
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "react/state-in-constructor": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js",
      "!test/ava-jsdom.js"
    ],
    "sources": [
      "src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "./test/ava-jsdom.js",
      "@babel/register",
      "css-modules-require-hook/preset"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx --ext .ts --ext tsx --cache --format=node_modules/eslint-formatter-pretty src run.js webpack.config.default.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "yarn run build:version && yarn run eslint && yarn run stylelint",
    "clean": "node run clean",
    "build": "cross-env NODE_ENV=production node run build",
    "build:debug": "cross-env NODE_ENV=development node run build --debug",
    "build:version": "cross-env NODE_ENV=production node run version",
    "start": "cross-env NODE_ENV=development node run",
    "test": "cross-env NODE_ENV=test ava",
    "test:coverage": "",
    "test:debug": "cross-env NODE_ENV=test ava --verbose test/pages/settings/AnidbImageSettings.test.js",
    "report": "nyc report --reporter=html",
    "coveralls": ""
  }
}
