    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": "off",
      "react/require-default-props": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "react/forbid-prop-types": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "prefer-promise-reject-errors": "off",
      "jsx-a11y/label-has-for": "off",
      "import/no-cycle": "off",
      "implicit-arrow-linebreak": "off",
      "react/jsx-one-expression-per-line": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "flow-typed/*",
    "build/*"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill",
      "css-modules-require-hook/preset"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx --cache --format=node_modules/eslint-formatter-pretty src run.js webpack.config.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "yarn run build:version && yarn run eslint && yarn run stylelint",
    "clean": "node run clean",
    "build": "cross-env NODE_ENV=production node run build",
    "build:debug": "cross-env NODE_ENV=development node run build --debug",
    "build:version": "cross-env NODE_ENV=production node run version",
    "start": "cross-env NODE_ENV=development node run",
    "test": "cross-env NODE_ENV=test ava",
    "test:coverage": "cross-env NODE_ENV=test nyc --extension .jsx ava",
    "test:debug": "cross-env NODE_ENV=test ava --verbose test/pages/settings/AnidbImageSettings.test.js",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  }
}
