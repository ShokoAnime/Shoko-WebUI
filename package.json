    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.3",
    "stylelint-config-standard": "^14.0.0",
    "url-loader": "^0.5.7"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "react/forbid-prop-types": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx components core pages utils main.jsx run.jsx webpack.config.js",
    "stylelint": "stylelint \"components/**/*.css\" \"pages/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "start": "node run",
    "test": "mocha test/.setup.js test/**/*-test.js"
  }
}
