    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.1",
    "redux-saga-testing": "^1.0.5",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.20.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": "off",
      "react/require-default-props": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "react/forbid-prop-types": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx src utils run.jsx webpack.config.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run build:version && npm run eslint && npm run stylelint",
    "clean": "node run clean",
    "build": "cross-env NODE_ENV=production node run build",
    "build:debug": "cross-env NODE_ENV=development node run build --debug",
    "build:version": "cross-env NODE_ENV=production node run version",
    "start": "cross-env NODE_ENV=development node run",
    "test": "ava",
    "test:coverage": "nyc ava",
    "report": "nyc report --reporter=html"
  }
}
