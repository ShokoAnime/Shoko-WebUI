    "node-fetch": "^2.6.7",
    "terser": "^5.14.2"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "customSyntax": "postcss-scss",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "color-function-notation": "legacy",
      "alpha-value-notation": "number",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2,
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "tailwind",
            "apply",
            "variants",
            "responsive",
            "screen"
          ]
        }
      ],
      "declaration-block-trailing-semicolon": null,
      "no-descending-specificity": null
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js",
      "!test/ava-jsdom.js"
    ],
    "sources": [
      "src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "./test/ava-jsdom.js",
      "@babel/register",
      "css-modules-require-hook/preset"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx --ext .ts --ext tsx --cache --format=node_modules/eslint-formatter-pretty src run.js webpack.config.default.js",
    "stylelint": "stylelint \"css/*.scss\"",
    "lint": "yarn run build:version && yarn run eslint && yarn run stylelint",
    "clean": "node run clean",
    "build": "cross-env NODE_ENV=production node run build",
    "build:debug": "cross-env NODE_ENV=development node run build --debug",
    "build:version": "cross-env NODE_ENV=production node run version",
    "start": "cross-env NODE_ENV=development node run"
  }
}
