{
  "name": "jmmserver-webui",
  "version": "0.3.2",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.8",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^2.2.2",
    "ejs": "^2.5.6",
    "enzyme": "^2.8.2",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.7.0",
    "history": "^4.6.1",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "object-path": "^0.11.4",
    "path-to-regexp": "^1.7.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^3.0.1",
    "prettysize": "0.1.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-mdl": "^1.10.2",
    "react-redux": "^5.0.4",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "redux-devtools": "^3.4.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.1",
    "redux-thunk": "^2.2.0",
    "roboto-npm-webfont": "0.0.2",
    "sessionstorage": "0.0.2",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-imports": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome-webpack2": "0.0.6",
    "jsdom": "^10.1.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "mocha": "^3.3.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^6.0.4",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": "off",
      "react/require-default-props": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/anchor-has-content": [
        2,
        {
          "components": [
            "Anchor"
          ]
        }
      ],
      "react/forbid-prop-types": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "color-hex-case": "lower",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx src utils run.jsx webpack.config.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "start": "node run",
    "test": "mocha test/.setup.js test/**/*-test.js"
  }
}
